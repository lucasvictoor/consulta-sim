<h1>Lista de Consultas</h1>

<table>
  <thead>
  <tr>
    <th>Data</th>
    <th>Médico</th>
    <th>Paciente</th>
  </tr>
  </thead>
  <tbody>
  <% @consultas.each do |consulta| %>
    <tr>
      <td><%= consulta.data %></td>
      <td><%= consulta.medico.nome %></td>
      <td><%= consulta.paciente.nome_completo %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Agendar Consulta', new_consulta_path %>
<%= link_to 'Voltar para à Home Page', root_path %>